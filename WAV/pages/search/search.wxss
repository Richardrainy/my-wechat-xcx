.act{
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
  padding: 0 20rpx;
}
.address, .organization, .category, .time, .date{
  border: 1px solid #f0f0f0;
  background-color: #fff;
  width: 90%;
  height: 30px;
  left:10px;
  margin-top: 20px;
  border-bottom: 1px solid #f0f0f0;

}
.key{
  font-family: PingFangSC;
  font-weight: 400;
  font-size: 20px;
  color: rgb(16, 16, 16);
  font-style: normal;
  letter-spacing: 0px;
  line-height: 28px;
  text-decoration: none;
}
.arrow-select{
  background-color: #fff;
  width: 20px;
  height: 20px;
  left: 10px;
}
.arrow-date{
  background-color: #fff;
  width: 20px;
  height: 20px;
  left: 10px;
}
button{
  margin-top: 70%;
}
.select-btn{
  width:60%;
  height:60rpx;
  color: #fdfdfd;
}
.value{
  color: #b2b2b2;
  left: 10px;
  line-height: 28px;
  margin-top: 1px;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
}